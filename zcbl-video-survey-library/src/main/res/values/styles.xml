<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ConnectionVideoTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <!--状态栏-->
        <item name="android:colorPrimaryDark">@color/title_bar_35A942</item>
        <!--光标-->
        <item name="android:colorPrimary">@color/title_bar_35A942</item>
    </style>

    <style name="WilddogTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <!--状态栏-->
        <item name="android:colorPrimaryDark">@color/title_bar_35A942</item>
        <!--光标-->
        <item name="android:colorPrimary">@color/title_bar_35A942</item>
    </style>

    <!-- H5加载弹出菊花框 -->
    <style name="loading_H5" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 2.beijing透明 -->
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">false</item>
        <!-- 模糊 -->
    </style>

</resources>